{
  "homepage":"https://jonathanoliveirarocha.github.io/dev-relate/",
  "name": "dev-relate",
  "version": "1.0.0",
  "description": "Website for annotating and consulting development studies",
  "main": "index.js",
  "scripts": {
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install",
    "install": "npm-run-all --sequential install-backend install-frontend",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm start",
    "start": "npm-run-all --parallel start-backend start-frontend",
    "predeploy": "cd frontend && npm run build",
    "deploy": "hg-pages -d frontend/dist" 
  },
  "devDependencies": {
    "gh-pages": "^6.1.0",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanoliveirarocha/dev-relate.git"
  },
